container_commands:
  "01":
    command: echo "Adding host domain to nginx access log format..."
  "02":
    command: perl -0777 -i -pe 's/log_format\s+main\s.*?;/log_format main \'\''\$remote_addr - \$remote_user [\$time_local] \$host "\$request" \$status \$body_bytes_sent "\$http_referer" "\$http_user_agent" "\$http_x_forwarded_for\"'\'';/gs'  ../config/#etc#nginx#nginx.conf
